declare -A folders

folders=(
	["ğŸ”— Codely"]="https://codely.com"
	["ğŸ”— Pro"]="https://pro.codely.com"
	["ğŸ“ Downloads"]="$HOME/Downloads"
	["ğŸ“ Desktop"]="$HOME/Desktop"
)

folder=$(printf "%s\n" "${!folders[@]}" | choose)

if [[ -n ${folders[$folder]} ]]; then
    open "${folders[$folder]}"
else
    echo "No folder selected"
fi